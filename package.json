{
	"name": "unagi_frontend_test",
	"private": true,
	"scripts": {
		"start": "run-p start:front start:back",
		"start:front": "parcel --port 8000 index.html",
		"start:back": "json-server --port 8001 db.json",
		"format": "prettier --write \"src/**/*.{js,jsx,json,md,ts,tsx,css}\"",
		"lint": "eslint src/**/*.{js,jsx,ts,tsx} --fix --max-warnings=0",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@types/node": "^20.14.8",
		"@types/react": "^17.0.25",
		"@types/react-dom": "^17.0.9",
		"@types/react-router-dom": "^5.3.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.5",
		"prettier": "^2.4.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^7.13.1",
		"@typescript-eslint/parser": "^7.13.1",
		"components": "^0.1.0",
		"eslint": "^8.9.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"json-server": "^0.16.3",
		"lib": "^5.1.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.3.0",
		"styled-components": "^6.1.11"
	},
	"lint-staged": {
		"*.{js,ts,tsx,jsx}": "npm run lint",
		"*.{js,css,md,html,json}": "npm run format"
	}
}
